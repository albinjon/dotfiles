# Close window
ctrl + cmd - w [
  "wezterm": skhd -k "cmd - w"
  "arc": skhd -k "cmd + shift - w"
  "finder": skhd -k "cmd - q"
  "*": skhd -k "cmd - w"
]

ctrl + cmd - n [
  "arc": skhd -k "cmd + shift - n"
]

# Close application
ctrl + cmd - q : skhd -k "cmd - q"

